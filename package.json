{
  "name": "typed-rest-api-client",
  "description": "A javascript/typescript library to connect to RESTful APIs",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "build": "tsc && node scripts/build.js",
    "clean": "rimraf dist",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "cross-env CI=true npm run test -- --ci --coverage",
    "test": "jest -c jest.config.ts",
    "lint": "eslint .",
    "prepack": "npm run clean"
  },
  "keywords": [
    "rest",
    "http",
    "typed-rest",
    "typescript-rest",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rosenkolev/typed-rest-api-client"
  },
  "main": "./dist/index.min.cjs",
  "module": "./dist/index.min.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Rosen Kolev",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "cross-env": "^7.0.3",
    "esbuild": "0.25.4",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-silent-reporter": "0.6.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.31.1"
  }
}
